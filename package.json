      "mysql": "^2.18.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.12",
      "validatorjs": "^3.22.1"
   },
   "devDependencies": {
      "@types/express": "^4.17.17",
      "@types/node": "^16.11.10",
